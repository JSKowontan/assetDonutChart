{
  "id": "com.autodeck.asset.donut.chart",
  "name": "Asset Breakdown Donut",
  "description": "Donut chart breakup of assets into liabilities and equities.",
  "newInstancePrefix": "assetDonutChart",
  "version": "1.0.0",
  "vendor": "autodeck.com",
  "eula": "autodeck.com",
  "license": "MIT",
  "icon": "https://cdn.jsdelivr.net/gh/JSKowontan/assetDonutChart@main/donutChartIcon.png",
  "imports": [
    "datasource",
    "planning-model"
  ],
  "webcomponents": [
    {
      "kind": "main",
      "tag": "asset-donut-chart",
      "url": "https://cdn.jsdelivr.net/gh/JSKowontan/assetDonutChart@e456dc3d0885f75639177f4ec5ce36da65fab180/assetDonutChart.js",
      "integrity": "sha256-fiw9mxvf8HLrUkPsoEBHy4wGWYbfvolTQidCf4QW9pU=",
      "ignoreIntegrity": false
    },
    {
      "kind": "builder",
      "tag": "asset-donut-builder",
      "url": "https://cdn.jsdelivr.net/gh/JSKowontan/assetDonutChart@e456dc3d0885f75639177f4ec5ce36da65fab180/assetDonutChart.js",
      "integrity": "sha256-fiw9mxvf8HLrUkPsoEBHy4wGWYbfvolTQidCf4QW9pU=",
      "ignoreIntegrity": false
    }
  ],
  "properties": {
    "chartTitle": { "type": "string", "default": "Asset Breakdown" },
    "centerLabel": { "type": "string", "default": "Asset" },
    "label1": { "type": "string", "default": "Liabilities" },
    "val1": { "type": "number", "default": 8371 },
    "color1": { "type": "string", "default": "#ed8b36" },
    "label2": { "type": "string", "default": "Equities" },
    "val2": { "type": "number", "default": 12356 },
    "color2": { "type": "string", "default": "#4472c4" }
  },
  "methods": {
    "setAssetData": {
      "description": "Set the values for Liabilities and Equities",
      "parameters": [
        { "name": "liabilities", "type": "number", "description": "Value 1" },
        { "name": "equities", "type": "number", "description": "Value 2" }
      ]
    }
  },
  "events": {
    "onDonutClick": {
      "description": "Triggered when a donut is clicked"
    }
  }
}
